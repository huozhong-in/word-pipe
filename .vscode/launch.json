    "version": "0.2.0",
    "configurations": [
        {
            "name": "macOS",
            "type": "dart",
            "request": "launch",
            "cwd": "app",
            "program": "lib/main.dart",
            "deviceId": "macOS",
            "flutterMode": "debug",
        },
        {
            "name": "iOS-simulator",
            "type": "dart",
            "request": "launch",
            "cwd": "app",
            "program": "lib/main.dart",
            "deviceId": "iPhone 14 Pro",
            "flutterMode": "debug",
        },
        {
            "name": "iOS-iphone",
            "type": "dart",
            "request": "launch",
            "cwd": "app",
            "program": "lib/main.dart",
            "deviceId": "Dio的iPhone",
            "flutterMode": "debug",
        },
        {
            "name": "iPad-simulator",
            "type": "dart",
            "request": "launch",
            "cwd": "app",
            "program": "lib/main.dart",
            "deviceId": "iPad Air (5th generation)",
            "flutterMode": "debug",
        },
        {
            "name": "Mobile-Web",
            "type": "dart",
            "request": "launch",
            "cwd": "app",
            "program": "lib/main.dart",
            "deviceId": "web-server",
            "toolArgs": [
                "--web-port 8080",
                "--web-hostname 0.0.0.0",
                "--web-renderer html"
            ]
        },
        {
            "name": "PC-Web(Html)",
            "cwd": "app",
            "request": "launch",
            "program": "lib/main.dart",
            "type": "dart",
            "flutterMode": "debug",
            "deviceId": "chrome",
            "toolArgs": [
                "--web-renderer=html"
            ]
        }
    ]
}